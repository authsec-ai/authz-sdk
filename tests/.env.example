# Environment variables for SDK testing
# Copy this file to .env and fill in your values

# Required: Admin authentication token
# Get this from: https://dev.api.authsec.dev or your local instance
ADMIN_TOKEN=your-admin-jwt-token-here

# Optional: API base URL (defaults to production)
API_BASE_URL=https://dev.api.authsec.dev

# Optional: Endpoint type for AdminHelper
# Options: "admin" or "enduser"
ENDPOINT_TYPE=admin

# Optional: Request timeout in seconds
ADMIN_TIMEOUT=10

# Test data - these are created/deleted during tests
TEST_USER_EMAIL=test-user@example.com
TEST_USER_ID=
TEST_ROLE_NAME=SDK Test Role
TEST_PERMISSION_RESOURCE=test_resource
TEST_PERMISSION_ACTION=test_action

# For login tests (optional)
TEST_LOGIN_EMAIL=
TEST_LOGIN_PASSWORD=
TEST_CLIENT_ID=
